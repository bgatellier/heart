    "@types/dotenv": "^6.1.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^10.14.10",
    "@typescript-eslint/eslint-plugin":"^5.32.0",
    "@typescript-eslint/parser":"^5.32.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
