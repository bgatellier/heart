{
  "name": "@fabernovel/heart-cli",
  "version": "1.2.0",
  "description": "Runner module of Heart, which starts an analysis by using a CLI",
  "homepage": "https://gitlab.com/fabernovel/heart",
  "bugs": {
    "url": "https://gitlab.com/fabernovel/heart/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/fabernovel/heart.git"
  },
  "license": "MIT",
  "contributors": [
    "Bastien Gatellier <bastien.gatellier@fabernovel.com>"
  ],
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "heart": "lib/index.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc",
    "format": "sort-package-json",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --follow-tags"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "@fabernovel/heart-core": "^1.5.0",
    "commander": "^2.20.0",
    "dotenv": "^8.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^10.14.10",
    "sort-package-json": "^1.22.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  }
}
